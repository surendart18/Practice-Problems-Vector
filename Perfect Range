Perfect Numbers from 1 to 100000


#include<stdio.h>
int main()
{
        for(int i=1;i<100000;i++)
        {
                int sum=0;
                for(int j=1;j<=i/2;j++)
                {
                        if(i%j==0)
                                sum+=j;
                }
                if(sum==i)
                        printf("%d ",i);
                }
        printf("\n");
}
